{
  "Framework": "qunit",
  "TestHarnessReferenceMode": "AMD",
  "TestHarnessLocationMode": "TestFileAdjacent",
  "TypeScriptModuleKind": "Normal",
  "SourceDirectory": "./",
  "OutDirectory": "../../js/test/",
  "References": [
    { "Path": "require.js" },
    { "Path": "qunit.js" },
    {
      "Path": "./typings/require.d.ts",
      "IncludeInTestHarness": "false"
    },
    {
      "Path": "./typings/qunit.d.ts",
      "IncludeInTestHarness": "false"
    }
  ],
  "Tests": [
    {
      "Includes": [ "**.ts" ],
      "Excludes": [ "**.d.ts" ]
    }
  ],
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  }
}
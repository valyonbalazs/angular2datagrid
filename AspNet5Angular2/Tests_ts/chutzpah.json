{
  "Framework": "qunit",
  "TestHarnessReferenceMode": "AMD",
  "TestHarnessLocationMode": "SettingsFileAdjacent",
  "TypeScriptModuleKind": "AMD",
  "References": [
    { "Path": "../node_modules/requirejs/require.js" },
    { "Path": "../node_modules/qunit/node_modules/qunitjs/qunit/qunit.js" },
    {
      "Path": "./typings/require.d.ts",
      "IncludeInTestHarness": "false"
    },
    {
      "Path": "./typings/qunit.d.ts",
      "IncludeInTestHarness": "false"
    },
    {
      "Path": "./tests",
      "IncludeInTestHarness": "false"
    }
  ],
  "Tests": [
    {
      "Includes": [ "*/tests/*.ts" ],
      "Excludes": [ "*/tests/*.d.ts" ]
    }
  ],
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  }
}